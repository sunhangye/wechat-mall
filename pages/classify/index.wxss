.container {
  position: relative;
  width: 100%;
  height: 100%;
  color: #939393;
}

.nav_left {
  display: inline-block;
  width: 25%;
  height: 100%;
  background-color: #F5F5F5;
}

.nav_left .nav_left_item {
  padding: 12px 10px;
  font-size: 14px;
  box-sizing: border-box;
  text-align: center;
  color: #939393;
}

.nav_left .active {
  background: #fff;
  border-left: 4px solid #f9f027;
  color: #333;
}

.nav_right {
  position: absolute;
  top: 48px;
  right: 0;
  flex: 1;
  width: 75%;
  height: 100%;
  padding: 10px;
  background: #fff;
  box-sizing: border-box;
}

.nav_right .nav_right_item{
  float: left;
  width: 33.33%;
  height: 80px;
  text-align: center;
}
.nav_right .nav_right_item image{
  width: 50px;
  height: 30px;
}
.nav_right .nav_right_item text{
  display: block;
  margin-top: 5px;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
